# AvaTax Configuration
AVATAX_ACCOUNT_ID=your_account_id_here
AVATAX_LICENSE_KEY=your_license_key_here

# Environment: 'sandbox' or 'production'
# Default is 'sandbox' if not specified
AVATAX_ENVIRONMENT=sandbox

# Company code in AvaTax (required for transactions)
AVATAX_COMPANY_CODE=DEFAULT

# Optional: Application identification
AVATAX_APP_NAME=AvaTax-MCP-Server
AVATAX_APP_VERSION=1.0.0
AVATAX_MACHINE_NAME=MCP-Server

# Optional: API timeout in milliseconds (default: 30000)
AVATAX_TIMEOUT=30000